// Lambda with positional args.
sum = (a, b){ println(a + b); };
sum(2, 3);

// Varargs must be named and come last.
var = (a, rest...){ println(a); println(rest.size); };
var(1);
var(1, 2, 3);

// Calling a table uses a method with the same name inside it.
obj = {a = 7, obj = (a){ println(this.a); println(a); }};
obj(9);

// Method calls bind this to the target table.
obj2 = {a = 3, foo = (x){ println(this.a + x); }};
obj2.foo(4);
