// Meta fields are accessed via string keys.
obj = (table){alpha = "test", beta = (number){1, 2, 3}};

// name/type/size/capacity come from meta.
println(obj.name);
println(obj.type);
println(obj.size);
println(obj.capacity);

// size can be changed (read-only fields will error).
obj.size = 1;
println(obj.size);
